{
  "name": "pkg-name",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "repository": {
    "url": "https://github.com/u3u/pkg-name.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "pnpm build",
    "test": "vitest --ui --coverage"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,tsx,md}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.0",
    "@u3u/eslint-config": "^2.12.0",
    "@u3u/prettier-config": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "bumpp": "^9.2.0",
    "dedent": "^1.5.1",
    "eslint": "^8.50.0",
    "lint-staged": "^14.0.1",
    "pkg-name": "link:",
    "prettier": "^3.0.3",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "packageManager": "pnpm@8.8.0",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node"
      ]
    }
  }
}
